{
  "name": "app-datepicker",
  "version": "3.0.0-alpha.0",
  "private": true,
  "description": "Material Design based datepicker element built with Polymer and LitElement",
  "keywords": [
    "app-datepicker",
    "datepicker",
    "lit-element",
    "lit-html",
    "node",
    "polymer",
    "typescript",
    "webcomponents"
  ],
  "homepage": "https://github.com/motss/app-datepicker",
  "repository": {
    "type": "git",
    "url": "git@github.com:motss/app-datepicker.git"
  },
  "license": "MIT",
  "author": {
    "name": "Rong Sen Ng",
    "email": "wes.ngrongsen@gmail.com",
    "url": "https://github.com/motss"
  },
  "main": "./index",
  "module": "./index.mjs",
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm && rm -rf dist/ && echo \"> [build] Done!\"",
    "build:cjs": "npm run lint && rm -rf dist/ && tsc -p tsconfig.json && mv dist/index.js dist/index.d.ts . && echo \"> [cjs] Done!\"",
    "build:esm": "npm run lint && rm -rf dist/ && tsc -p tsconfig.esm.json && mv dist/index.js ./index.mjs && echo \"> [esm] Done!\"",
    "lint": "tslint --project tsconfig.json --config tslint.prod.json --format stylish 'src/**/*.ts*'",
    "lint:debug": "tslint --project tsconfig.json --config tslint.json --format stylish 'src/**/*.ts*'",
    "prepublishOnly": "npm run lint && npm run build"
  },
  "dependencies": {
    "@polymer/iron-iconset-svg": "^3.0.0-pre.25",
    "@polymer/iron-selector": "^3.0.0-pre.25",
    "@polymer/lit-element": "^0.6.0-dev.5",
    "@polymer/paper-button": "^3.0.0-pre.25",
    "@polymer/paper-icon-button": "^3.0.0-pre.25"
  },
  "devDependencies": {
    "@messageflow/tslint-config": "^1.1.0",
    "@webcomponents/webcomponentsjs": "^2.1.1",
    "tslint": "^5.11.0",
    "typescript": "^3.0.3"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.5.1"
  },
  "flat": true
}
